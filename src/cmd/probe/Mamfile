prev probe
setv mode -Dmode="u+s,+x"
exec - test '' = 'probe' || ${STDCMP} 2>/dev/null -s probe ${INSTALLROOT}/bin/probe || { ${STDMV} ${INSTALLROOT}/bin/probe ${INSTALLROOT}/bin/probe.old 2>/dev/null || true; ${STDCP} probe ${INSTALLROOT}/bin/probe && chmod u+s,+x ${INSTALLROOT}/bin/probe ;}
done ${INSTALLROOT}/bin/probe generated
