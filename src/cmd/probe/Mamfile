done probe.o generated
bind -last
exec - ${CC} ${CCLDFLAGS} ${mam_cc_FLAGS} ${CCFLAGS} ${LDFLAGS} ${mam_cc_L+-L${INSTALLROOT}/lib} -o probe probe.o ${mam_libast} ${mam_libast}
done probe generated
make ${INSTALLROOT}/lib/probe
exec - if silent test ! -d ${INSTALLROOT}/lib/probe
exec - then mkdir -p ${INSTALLROOT}/lib/probe
exec - fi
done ${INSTALLROOT}/lib/probe generated
make ${INSTALLROOT}/lib/probe/probe
prev ${INSTALLROOT}/lib/probe
prev probe
setv mode -Dmode="u+s,+x"
exec - test '' = 'probe' || ${STDCMP} 2>/dev/null -s probe ${INSTALLROOT}/lib/probe/probe || { ${STDMV} ${INSTALLROOT}/lib/probe/probe ${INSTALLROOT}/lib/probe/probe.old 2>/dev/null || true; ${STDCP} probe ${INSTALLROOT}/lib/probe/probe && chmod u+s,+x ${INSTALLROOT}/lib/probe/probe ;}
done ${INSTALLROOT}/lib/probe/probe generated
make ${INSTALLROOT}/bin
