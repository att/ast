make test
make test.sort
make sort.tst
meta sort.tst %.rt>%.tst sort.rt sort
make sort.rt
done sort.rt
exec - if [[ "1" || "sort.rt" -nt "sort.tst" ]]
exec - then mktest --style=regress sort.rt > sort.tst
exec - fi
done sort.tst generated
exec - regress sort.tst
done test.sort virtual
done test dontcare virtual
