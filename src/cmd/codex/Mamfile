done codex.o generated
bind -lcodex
bind -last
exec - ${CC} ${CCLDFLAGS} ${mam_cc_FLAGS} ${CCFLAGS} ${LDFLAGS} ${mam_cc_L+-L${INSTALLROOT}/lib} -o codex codex.o ${mam_libcodex} ${mam_libast}
done codex generated
make ${INSTALLROOT}/bin
exec - if silent test ! -d ${INSTALLROOT}/bin
exec - then mkdir -p ${INSTALLROOT}/bin
exec - fi
done ${INSTALLROOT}/bin generated
make ${INSTALLROOT}/bin/codex
prev ${INSTALLROOT}/bin
prev codex
exec - test '' = 'codex' || ${STDCMP} 2>/dev/null -s codex ${INSTALLROOT}/bin/codex || { ${STDMV} ${INSTALLROOT}/bin/codex ${INSTALLROOT}/bin/codex.old 2>/dev/null || true; ${STDCP} codex ${INSTALLROOT}/bin/codex ;}
done ${INSTALLROOT}/bin/codex generated
done install virtual
make test
make test.codex
prev codex
make codex.tst
done codex.tst
exec - regress codex.tst codex
done test.codex virtual
done test dontcare virtual
