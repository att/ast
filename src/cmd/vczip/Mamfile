done vczip generated
make vcunzip
prev vczip
exec - if silent test -f "vcunzip"
exec - then rm -f vcunzip
exec - fi
exec - ln vczip vcunzip
done vcunzip generated
make ${INSTALLROOT}/bin
exec - if silent test ! -d ${INSTALLROOT}/bin
exec - then mkdir -p ${INSTALLROOT}/bin
exec - fi
done ${INSTALLROOT}/bin generated
make ${INSTALLROOT}/bin/vczip
prev ${INSTALLROOT}/bin
prev vczip
exec - test '' = 'vczip' || ${STDCMP} 2>/dev/null -s vczip ${INSTALLROOT}/bin/vczip || { ${STDMV} ${INSTALLROOT}/bin/vczip ${INSTALLROOT}/bin/vczip.old 2>/dev/null || true; ${STDCP} vczip ${INSTALLROOT}/bin/vczip ;}
done ${INSTALLROOT}/bin/vczip generated
