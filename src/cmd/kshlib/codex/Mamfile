done lib.o generated
exec - ${AR} rc libkshcodex.a codex.o lib.o
exec - (ranlib libkshcodex.a) >/dev/null 2>&1 || true
done libkshcodex.a generated
done ../../codex/codex
done install virtual
make test
make test.codex
make codex.tst
meta codex.tst %.rt>%.tst codex.rt codex
make codex.rt
done codex.rt
exec - if [[ "1" || "codex.rt" -nt "codex.tst" ]]
exec - then mktest --style=regress codex.rt > codex.tst
exec - fi
done codex.tst generated
exec - regress codex.tst ${SHELL}
done test.codex virtual
done test dontcare virtual
