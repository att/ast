make test
make test.dbm_t
make dbm_t.tst
meta dbm_t.tst %.rt>%.tst dbm_t.rt dbm_t
make dbm_t.rt
done dbm_t.rt
exec - if [[ "1" || "dbm_t.rt" -nt "dbm_t.tst" ]]
exec - then mktest --style=regress dbm_t.rt > dbm_t.tst
exec - fi
done dbm_t.tst generated
exec - regress dbm_t.tst ${SHELL}
done test.dbm_t virtual
done test dontcare virtual
