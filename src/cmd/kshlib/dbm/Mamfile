done libkshdbm.a generated
done dbm virtual
done install virtual
make test
make test.dbm
make dbm.tst
meta dbm.tst %.rt>%.tst dbm.rt dbm
make dbm.rt
done dbm.rt
exec - if [[ "1" || "dbm.rt" -nt "dbm.tst" ]]
exec - then mktest --style=regress dbm.rt > dbm.tst
exec - fi
done dbm.tst generated
exec - regress dbm.tst ${SHELL}
done test.dbm virtual
done test dontcare virtual
