make test.script
make testsort
make testsort.sh
done testsort.sh
meta testsort %.sh>% testsort.sh testsort
prev testsort.sh
exec - case static,port:$OPTIND:$RANDOM in
exec - ?*:*:*|*::*|*:*:$RANDOM)
exec - ;;
exec - *) if ENV= LC_ALL=C x= $SHELL -nc '[[ a || b ]] && : ${list[level]}  !(pattern)' 2>/dev/null
exec - then if grep '### .*archaic.* ###' >/dev/null
exec - then : testsort contains archaic constructs :
exec - else ENV= LC_ALL=C $SHELL -n testsort.sh
exec - fi
exec - fi
exec - ;;
exec - esac
exec - case '${mam_cc_SHELLMAGIC}' in
exec - "") case 14 in
exec - 0) cp testsort.sh testsort
exec - ;;
exec - *) {
exec - i=`(read x; echo $x) < testsort.sh`
exec - case $i in
exec - '#!'*|*'||'*|':'*|'":"'*|"':'"*) echo "$i" ;;
exec - esac
exec - cat - testsort.sh <<'!'
exec - TESTCC='${CC}'
exec - !
exec - } > testsort
exec - ;;
exec - esac
exec - ;;
exec - *) cat - testsort.sh > testsort <<'!'
exec - ${mam_cc_SHELLMAGIC}
exec - TESTCC='${CC}'
exec - !
exec - ;;
exec - esac
exec - silent test -w testsort -a -x testsort || chmod u+w,+x testsort
done testsort virtual
exec - set +x; (ulimit -c 0) >/dev/null 2>&1 && ulimit -c 0; set -x
exec - testsort sort
done test.script virtual
done test dontcare virtual
