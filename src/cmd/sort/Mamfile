done ${INSTALLROOT}/bin generated
make ${INSTALLROOT}/bin/sort
prev ${INSTALLROOT}/bin
prev sort
exec - test '' = 'sort' || ${STDCMP} 2>/dev/null -s sort ${INSTALLROOT}/bin/sort || { ${STDMV} ${INSTALLROOT}/bin/sort ${INSTALLROOT}/bin/sort.old 2>/dev/null || true; ${STDCP} sort ${INSTALLROOT}/bin/sort ;}
done ${INSTALLROOT}/bin/sort generated
