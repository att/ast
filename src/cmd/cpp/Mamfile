meta cpp.o %.c>%.o cpp.c cpp
prev cpp.c
exec - ${CC} ${mam_cc_FLAGS} ${CCFLAGS} -I${PACKAGE_ast_INCLUDE} -D_PACKAGE_ast -c cpp.c
done cpp.o generated
bind -lpp
bind -last
exec - ${CC} ${CCLDFLAGS} ${mam_cc_FLAGS} ${CCFLAGS} ${LDFLAGS} ${mam_cc_L+-L${INSTALLROOT}/lib} -o cpp cpp.o ${mam_libpp} ${mam_libast}
done cpp generated
make acpp
prev cpp
exec - if silent test -f "acpp"
exec - then rm -f acpp
exec - fi
exec - ln cpp acpp
done acpp generated
make ${INSTALLROOT}/bin
exec - if silent test ! -d ${INSTALLROOT}/bin
exec - then mkdir -p ${INSTALLROOT}/bin
exec - fi
done ${INSTALLROOT}/bin generated
make ${INSTALLROOT}/bin/cpp
prev ${INSTALLROOT}/bin
prev cpp
exec - test '' = 'cpp' || ${STDCMP} 2>/dev/null -s cpp ${INSTALLROOT}/bin/cpp || { ${STDMV} ${INSTALLROOT}/bin/cpp ${INSTALLROOT}/bin/cpp.old 2>/dev/null || true; ${STDCP} cpp ${INSTALLROOT}/bin/cpp ;}
done ${INSTALLROOT}/bin/cpp generated
make ${INSTALLROOT}/man/man1
exec - if silent test ! -d ${INSTALLROOT}/man/man1
exec - then mkdir -p ${INSTALLROOT}/man/man1
exec - fi
done ${INSTALLROOT}/man/man1 generated
make ${INSTALLROOT}/man/man1/cpp.1
prev ${INSTALLROOT}/man/man1
make cpp.1
done cpp.1
exec - test '' = 'cpp.1' || ${STDCMP} 2>/dev/null -s cpp.1 ${INSTALLROOT}/man/man1/cpp.1 || { ${STDMV} ${INSTALLROOT}/man/man1/cpp.1 ${INSTALLROOT}/man/man1/cpp.1.old 2>/dev/null || true; ${STDCP} cpp.1 ${INSTALLROOT}/man/man1/cpp.1 ;}
done ${INSTALLROOT}/man/man1/cpp.1 generated
make ${INSTALLROOT}/lib
exec - if silent test ! -d ${INSTALLROOT}/lib
exec - then mkdir -p ${INSTALLROOT}/lib
exec - fi
done ${INSTALLROOT}/lib generated
make ${INSTALLROOT}/lib/cpp
prev ${INSTALLROOT}/lib
prev cpp
exec - test '' = 'cpp' || ${STDCMP} 2>/dev/null -s cpp ${INSTALLROOT}/lib/cpp || { ${STDMV} ${INSTALLROOT}/lib/cpp ${INSTALLROOT}/lib/cpp.old 2>/dev/null || true; ${STDCP} cpp ${INSTALLROOT}/lib/cpp ;}
done ${INSTALLROOT}/lib/cpp generated
make ${INSTALLROOT}/lib/acpp
prev ${INSTALLROOT}/lib/cpp
exec - if silent test -f "${INSTALLROOT}/lib/acpp"
exec - then rm -f ${INSTALLROOT}/lib/acpp
exec - fi
exec - ln ${INSTALLROOT}/lib/cpp ${INSTALLROOT}/lib/acpp
done ${INSTALLROOT}/lib/acpp generated
done install virtual
make test
make test.cpp
prev cpp
make cpp.tst
done cpp.tst
exec - regress cpp.tst cpp
done test.cpp virtual
done test dontcare virtual
