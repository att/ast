exec - test '' = 'pax' || ${STDCMP} 2>/dev/null -s pax ${INSTALLROOT}/bin/pax || { ${STDMV} ${INSTALLROOT}/bin/pax ${INSTALLROOT}/bin/pax.old 2>/dev/null || true; ${STDCP} pax ${INSTALLROOT}/bin/pax ;}
done ${INSTALLROOT}/bin/pax generated
