exec - } > 1.${COTEMP}.c
exec - ${STDED} ${STDEDFLAGS} 1.${COTEMP}.c <<'!'
exec - g/^#define N_/m0
exec - v/^#define [NT]_/d
exec - 1,$s/^#define \(T_[A-Z0-9_]*\).*/&\
exec - printf(" \1 %d", \1);/
