exec - 0a
exec - #include <stdio.h>
exec - int
exec - main(argc, argv)
exec - int argc;
exec - char** argv;
exec - {
exec - printf("%%token /*generated from ppkey.h*/");
exec - .
exec - $a
exec - printf("\n");
exec - return 0;
exec - }
exec - .
exec - w
exec - q
exec - !
exec - ${CC} -o ${COTEMP}.exe 1.${COTEMP}.c
exec - ./${COTEMP}.exe > ppkey.yacc
exec - rm -f 1.${COTEMP}.c ${COTEMP}.exe
